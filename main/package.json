{
    "name": "isv",
    "version": "1.0.0",
    "license": "MIT",
    "description": "Infinite Virtual Success Homepage by Hima Balde.",
    "repository": {
        "url": "https://github.com/bahim22/isv"
    },
    "homepage": ".",
    "keywords": [
        "React",
        "MUI",
        "eslint",
        "prettier",
        "React",
        "Bahim22",
        "EvoSync Solutions",
        "JavaScript"
    ],
    "author": "EvoSync Solutions",
    "bugs": {
        "url": "https://github.com/bahim22/isv/issues"
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "markdownlint": "markdownlint-cli2 \"**/*.md\"",
        "lint:pretest": "npx eslint \"src/**/*.{js,jsx}\"",
        "test:lint-rules": "npx eslint src/components/*.js --config .eslintrc.js",
        "test:lint": "npx eslint src/components/*.js --config eslint.config.js",
        "lint:fix": "npx eslint \"src/**/*.{js,jsx}\" --fix",
        "prettier:check": "npx prettier --check \"src/**/*.{js,jsx}\"",
        "prettier:write": "npx prettier --write \"src/**/*.{js,jsx}\"",
        "test:espretty": "npx eslint-config-prettier \"src/**/*.{js,jsx}\"",
        "clean": "rm build/* && rm -rf build/**/*",
        "clean2": "rm -rf build/* && rm -rf build/js && rm -rf build/static && rm -rf build/main && rm -rf build"
    },
    "dependencies": {
        "@emotion/react": "latest",
        "@emotion/styled": "latest",
        "@formspree/react": "^2.5.1",
        "@mui/icons-material": "latest",
        "@mui/material": "next",
        "axios": "^1.7.2",
        "dotenv": "^16.4.5",
        "env-cmd": "^10.1.0",
        "react": "latest",
        "react-dom": "latest",
        "react-scripts": "latest"
    },
    "devDependencies": {
        "@eslint/js": "^9.3.0",
        "eslint": "latest",
        "eslint-config-prettier": "latest",
        "eslint-plugin-import": "latest",
        "eslint-plugin-jsx-a11y": "latest",
        "eslint-plugin-prettier": "latest",
        "eslint-plugin-react": "latest",
        "prettier": "latest"
    },
    "engines": {
        "node": ">=20",
        "npm": ">= 10"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}
